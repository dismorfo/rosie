<div id="zone-branding" class="zone zone-branding clearfix container-12">
  <div class="grid-12 region region-branding" id="region-branding">
    <div class="region-inner region-branding-inner">
      <div class="branding-data clearfix">
        <div class="logo-img">
          <a href="{{baseurl}}" rel="home" title="{{appName}}" class="active">
            <img src="{{baseurl 'images/the-real-rosie-the-riveter-logo.png'}}" alt="{{appName}}" id="logo" />
          </a>
        </div>
        <hgroup class="site-name-slogan element-invisible">
          <h1 class="site-name element-invisible"><a href="{{baseurl}}" title="Home" class="active">{{appName}}</a></h1>
        </hgroup>
      </div>
      {{> search }}
    </div>
  </div>
</div>
<div id="zone-menu" class="zone zone-menu clearfix container-12">
  <div class="grid-12 region region-menu" id="region-menu">
    <div class="region-inner region-menu-inner">
      <div class="block block-system block-menu block-main-menu block-system-main-menu odd block-without-title" id="block-system-main-menu">
        <div class="block-inner clearfix">
          <div class="content clearfix">
            <ul class="menu">
              <li class="first leaf"><a href="{{baseurl 'about'}}" title="About the Real Rosie the Riveter Project">About the Project</a></li>
              <li class="leaf"><a href="{{baseurl 'interviews'}}" title="See a list of the all the video interviews">Browse All Videos</a></li>
              <li class="last leaf"><a href="{{baseurl 'related-resources'}}" title="Related Resources at the Tamiment Library">Related Resources</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
